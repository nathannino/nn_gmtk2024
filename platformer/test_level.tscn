[sub_resource type="GDScript" id="GDScript_mbk2j"]
script/source = "extends Node2D

@export var camera : Camera2D
@export var player : CharacterBody2D
@export var notificationlabl : RichTextLabel
@export var notification_timeout : float
var current_notification = 0
var is_notification_active = false

func _process(delta):
	if current_notification > 0 :
		current_notification -= delta
	elif is_notification_active :
		clear_notification()

signal tool_placed

signal add_tool(p_tool : gmtk_tool)

func disable_camera() :
	camera.enabled = false
	pass

func enable_camera() :
	camera.enabled = true
	pass

func reset_player_pos() :
	player.position = Vector2.ZERO

func start_tool_preview(tool : gmtk_tool) :
	player.tool_preview.init_preview(tool)

func clear_notification() :
	notificationlabl.clear()
	is_notification_active = false
	current_notification = 0

func start_notification_tool(tool : gmtk_overworld_tool) :
	clear_notification()
	is_notification_active = true
	current_notification = notification_timeout
	notificationlabl.add_text(\"Unlocked \")
	notificationlabl.add_image(tool.sprite)
	notificationlabl.add_text(\" \" + tool.name)
	pass

func _input(event):
	if event.is_action_pressed(\"cf_confirm\") :
		if not player.current_tool_ground == null :
			player.current_tool_ground.queue_free()
			player.current_tool_ground = null
			
			add_tool.emit(player.current_tool_res)
			player.current_tool_res = null
			player.hide_indicator()

#var current_tool_ground : Node2D
#var current_tool_res : gmtk_tool

func _on_player_platformer_tool_placed():
	tool_placed.emit()
	pass # Replace with function body.
"

